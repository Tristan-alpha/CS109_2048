<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane prefHeight="148.0" prefWidth="447.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.cs109project.UserPrompt">
   <children>
      <HBox layoutX="94.0" layoutY="26.0" spacing="15.0">
         <children>
            <Label prefHeight="28.8" text="Username" />
            <TextField fx:id="textfieldUsername" promptText="Input your username..." />
         </children>
      </HBox>
      <HBox layoutX="132.0" layoutY="74.0" spacing="20.0">
         <children>
            <Button fx:id="buttonSearch" mnemonicParsing="false" onMouseClicked="#search" text="Search" />
            <Button fx:id="buttonBack" mnemonicParsing="false" onMouseClicked="#backToLogin" text="Back to Login" />
         </children>
      </HBox>
   </children>
</AnchorPane>
